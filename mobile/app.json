{
  "expo": {
    "name": "ButterNovel",
    "slug": "butternovel",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "butternovel",
    "userInterfaceStyle": "dark",
    "splash": {
      "backgroundColor": "#3b82f6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.butternovel.app"
    },
    "android": {
      "package": "com.butternovel.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
